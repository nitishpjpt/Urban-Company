{
  "name": "Easy2Get",
  "slug": "batting",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/images/easy2get-logo.png",
  "scheme": "myapp",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/easy2get-logo.png",
      "backgroundColor": "#000000"
    },
    "package": "com.nitishpjpt.Batting"
  },
  "web": {
    "bundler": "metro"
  },
  "plugins": [
    "expo-router",
   
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/easy2get-logo.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {
      "origin": false
    },
    "eas": {
      "projectId": "b0cff82e-af39-4634-87e0-85012a1e6909"
    }
  },
  "owner": "nitishpjpt"
}
